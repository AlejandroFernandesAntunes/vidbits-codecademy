<div class="contents-container">
  <a id='create-video' href='create'>Create a video</a>
  <div id="videos-container" class="contents-container"></div>
  {{#each videos as |video|}}
    <div class="video-card">
      <div class="video-title">
        {{video.title}}
      </div>
    </div>
  {{/each}}
</div>